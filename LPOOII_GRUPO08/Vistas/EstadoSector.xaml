<Window x:Class="Vistas.EstadoSector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EstadoSector" Height="300" Width="300">
    <Window.Resources>
        <XmlDataProvider x:Key="Tiempos" Source="Tiempos.xml"  XPath="/tiempos/tiempo" ></XmlDataProvider>
    </Window.Resources>
    <Grid Background="Black">
                <ComboBox Width="150" Height="30" Name="comboBoxTiempos" ItemsSource="{Binding Source={StaticResource Tiempos}}" DisplayMemberPath="@duracion" SelectedValuePath="@duracion" Margin="67,165,61,66" />
    </Grid>
</Window>
